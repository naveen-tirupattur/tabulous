{
  "manifest_version": 3,
  "name": "Tabulous",
  "version": "1.0",
  "description": "A Chrome extension that displays a button in the tab.",
  "action": {
    "default_popup": "src/main/ui/popup.html",
    "default_icon": {
      "16": "images/icon-tabulous-50.png",
      "48": "images/icon-tabulous-50.png",
      "128": "images/icon-tabulous-100.png"
    }
  },
  "icons": {
    "16": "images/icon-tabulous-50.png",
    "48": "images/icon-tabulous-50.png",
    "128": "images/icon-tabulous-100.png"
  }
}